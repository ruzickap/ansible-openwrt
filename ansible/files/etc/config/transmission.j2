config transmission
	option enabled 1
	option config_dir '{{ usb_disk_mount_path }}/torrents/config'
	option config_overwrite '1'
	option user 'transmission'
	option group 'transmission'
	option mem_percentage 50
	option nice 10
	option web_home ''
	option alt_speed_down 99999
	option alt_speed_enabled true
	option alt_speed_time_begin  60
	option alt_speed_time_day 127
	option alt_speed_time_enabled true
	option alt_speed_time_end 420
	option alt_speed_up 10
	option bind_address_ipv4 '0.0.0.0'
	option bind_address_ipv6 '::'
	option blocklist_enabled true
	option blocklist_url '{{ transmission_blocklist_url }}'
	option cache_size_mb 2
	option dht_enabled true
	option download_dir '{{ usb_disk_mount_path }}/torrents/torrents-completed'
	option download_queue_enabled true
	option download_queue_size 4
	option encryption 1
	option idle_seeding_limit 30
	option idle_seeding_limit_enabled false
	option incomplete_dir '{{ usb_disk_mount_path }}/torrents/torrents-incomplete'
	option incomplete_dir_enabled true
	option lazy_bitfield_enabled true
	option lpd_enabled false
	option message_level 1
	option peer_congestion_algorithm ''
	option peer_limit_global 240
	option peer_limit_per_torrent 60
	option peer_port 51413
	option peer_port_random_high 65535
	option peer_port_random_low 49152
	option peer_port_random_on_start false
	option peer_socket_tos 'default'
	option pex_enabled true
	option port_forwarding_enabled true
	option preallocation 1
	option prefetch_enabled true
	option queue_stalled_enabled true
	option queue_stalled_minutes 30
	option ratio_limit 0
	option ratio_limit_enabled true
	option rename_partial_files true
	option rpc_authentication_required false
	option rpc_bind_address '127.0.0.1'
	option rpc_enabled true
	option rpc_host_whitelist '127.0.0.1'
	option rpc_host_whitelist_enabled false
	option rpc_password ''
	option rpc_port 9091
	# http://192.168.1.1:9091/web/
	option rpc_url '/'
	option rpc_username ''
	option rpc_whitelist '127.0.0.1'
	option rpc_whitelist_enabled false
	option scrape_paused_torrents_enabled true
	option script_torrent_done_enabled true
	option script_torrent_done_filename '/etc/torrent-done.sh'
	option seed_queue_enabled false
	option seed_queue_size 10
	option speed_limit_down {{ transmission_speed_limit_down }}
	option speed_limit_down_enabled true
	option speed_limit_up {{ transmission_speed_limit_up }}
	option speed_limit_up_enabled true
	option start_added_torrents true
	option trash_original_torrent_files true
	option umask 18
	option upload_slots_per_torrent 5
	option utp_enabled true
	option scrape_paused_torrents true
	option watch_dir_enabled false
	option watch_dir ''
